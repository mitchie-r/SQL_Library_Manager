extends layout

block content
  h1 Page Not Found
  h2 Sorry, we couldn't find the page you were looking for. 
  h3= error.status

  section.page_404
    .container
      .row
        .col-sm-12 
          .col-sm-10.col-sm-offset-1.text-center
            .four_zero_four_bg
              h1.text-center 404
            .contant_box_404
              h3.h2 Looks like you're lost
              p the page you are looking for not available!
              a.link_404(href='/') Go to Home
      img.main-error(src="images/404-error.jpg", alt="404 Error Image") 
